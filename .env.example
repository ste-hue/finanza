# File: .env.example
# Copia questo file in .env e inserisci le tue credenziali Supabase.
# Queste variabili sono usate sia dal backend che dallo script ./orti.

# Trovi l'URL del progetto in "Project Settings" > "API" nel tuo dashboard Supabase
SUPABASE_URL="https://your-project-id.supabase.co"

# Trovi la Anon Key (pubblica) in "Project Settings" > "API"
SUPABASE_ANON_KEY="your-public-anon-key"

# Trovi la stringa di connessione in "Project Settings" > "Database"
# Assicurati di SOSTITUIRE [YOUR-PASSWORD] con la password del tuo database.
DATABASE_URL="postgresql://postgres:[YOUR-PASSWORD]@db.your-project-id.supabase.co:5432/postgres"
